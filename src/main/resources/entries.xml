<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<entries>

	<entry name="Article">
		<field name="journal" required="true" type="select" maxLength="512" />
		<field name="volume" maxLength="2" />
		<field name="number" maxLength="2" />
		<field name="year" required="true" maxLength="4" />
		<field name="month" type="select" maxLength="3" />
		<field name="pages" maxLength="32" />
	</entry>


	<entry name="Book">
		<field name="series" maxLength="512" />
		<field name="volume" maxLength="2" />
		<field name="publisher" required="true" type="select" maxLength="512" />
		<field name="isbn" lbl="ISBN" tag="ISBN" maxLength="32" />
		<field name="year" required="true" maxLength="4" />
		<field name="month" type="select" maxLength="3" />
	</entry>

	<entry name="Booklet">
		<field name="year" maxLength="4" />
		<field name="month" type="select" maxLength="3" />
		<field name="address" maxLength="512" />
		<field name="howpublished" maxLength="512" />
	</entry>

	<entry name="Inbook">
		
		<field name="year" required="true" maxLength="4" />
		<field name="month" type="select" maxLength="3" />
		<field name="series" maxLength="512" />
		<field name="volume" maxLength="2" />
		<field name="number" maxLength="2" />
		<field name="chapter" maxLength="2" />
		<field name="pages" maxLength="32" />
		<field name="publisher" required="true" type="select" maxLength="512" />
		
	</entry>

	<entry name="Incollection">
	
		<field name="chapter" maxLength="2" />
		<field name="pages" maxLength="32" />
		<field name="book" type="select" required="true"
			maxLength="32" />
		<field name="note" maxLength="1024" />
	</entry>
	<entry name="Inproceedings">
		<field name="booktitle" maxLength="512" />
		<field name="pages" maxLength="32" />
		<field name="proceedings"  type="select" required="true"
			maxLength="32" />
	</entry>
	<entry name="Manual">
		<field name="edition" maxLength="32" />
		<field name="year" maxLength="4" />
		<field name="month" type="select" maxLength="3" />
		<field name="organization" maxLength="512" />
		<field name="address" maxLength="512" />
		
	</entry>

	<entry name="Masterthesis">
		<field name="edition" maxLength="512" />
		<field name="year" required="true" maxLength="4" />
		<field name="month" type="select" maxLength="3" />
		<field name="school" required="true" maxLength="512" />
		<field name="address" maxLength="512" />
	</entry>


	<entry name="Misc">
		<field name="edition" maxLength="512" />
		<field name="year" maxLength="4" />
		<field name="month" type="select" maxLength="3" />
		<field name="howpublished" maxLength="512" />
	</entry>
	<entry name="Phdthesis">
		<field name="edition" maxLength="512" />
		<field name="year" required="true" maxLength="4" />
		<field name="month" type="select" maxLength="3" />
		<field name="school" required="true" maxLength="512" />
		<field name="address" maxLength="512" />
	</entry>

	<entry name="Proceedings">
		<field name="series" maxLength="512" />
		<field name="volume" maxLength="2" />
		<field name="number" maxLength="2" />
		<field name="year" required="true" maxLength="4" />
		<field name="month" type="select" maxLength="3" />
		<field name="publisher" type="select" maxLength="512" />
		<field name="organization" maxLength="512" />
		<field name="address" maxLength="512" />
		<field name="isbn" lbl="ISBN" tag="ISBN" maxLength="32" />
	</entry>

	<entry name="Techreport">
		<field name="type" maxLength="32" />
		<field name="number" maxLength="2" />
		<field name="year" required="true" maxLength="4" />
		<field name="month" type="select" maxLength="3" />
		<field name="institution" required="true" maxLength="512" />
		<field name="address" maxLength="512" />
	</entry>

	<entry name="Unpublished">
		<field name="year" maxLength="4" />
		<field name="month" type="select" maxLength="3" />
	</entry>

</entries>